{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./",
        "rootDir": "./",
        "declaration": false,
        "sourceMap": true
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"]
}
